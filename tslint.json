{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "member-access": false,
    "no-empty": false,
    "semicolon": [true, "never"]
  }
}
